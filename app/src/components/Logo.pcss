@import '../config.pcss';

.logoContainer {
  margin: 16px auto;
  width: 122px;
  height: 34px;

  @media(--medium-viewport) {
	margin-top: 40px;
	margin-bottom: 40px;
  }
}
.logo {
  display: block;
  background-image: svg-load('ucr-logo.svg');
  width: 100%;
  height: 100%;
  background-repeat: no-repeat;
  background-position: left center;
  background-size: 100% 100%;
}
